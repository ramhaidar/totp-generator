{
  "name": "totp-generator",
  "homepage": "https://ramhaidar.github.io/totp-generator",
  "author": "Ram Haidar <ramhaidar@gmail.com> (https://github.com/ramhaidar)",
  "repository": "github:ramhaidar/totp-generator",
  "private": true,
  "license": "MIT",
  "scripts": {
    "pre-commit": "npm run rebuild && npm run lint",
    "http-server": "http-server ./docs -p 8080 -c-1",
    "build": "npm run build-page && npm run webpack && npm run fix-references",
    "build-page": "ts-node-script src/generate-page.ts",
    "clean": "shx rm -rf docs/* **/*generated.*",
    "lint": "eslint src/ts -c src/config/.eslintrc.json --ext .ts",
    "lint-fix": "eslint src/ts -c src/config/.eslintrc.json --ext .ts --fix",
    "rebuild": "npm run clean && npm run build",
    "fix-references": "node scripts/fix-references.js",
    "webpack": "webpack --config src/config/webpack.config.js",
    "webpack-watch": "webpack --config src/config/webpack.config.js --watch"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.130",
    "@types/totp-generator": "^0.0.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "fs-extra": "^11.3.3",
    "http-server": "^14.1.1",
    "shx": "^0.3.4",
    "totp-generator": "^0.0.14",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^5.1.4",
    "webpage-templates": "github:piellardj/webpage-templates"
  }
}